<h1>New Student Form</h1>
<br>
<%=form_tag "/students" do%>
  
  <%= label_tag "First Name"%>
  <br>
  <%= text_field_tag "student[first_name]"%>
  <br>
  <br>
  <%= label_tag "Last Name"%>
  <br>
  <%= text_field_tag "student[last_name]"%>
  <br>
  <br>
  <%=submit_tag "Submit Student"%>

<%end%>

<%= params.inspect%>